{
  "name": "call tracking",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch --entryFile api/main",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/api/main",
    "start:api": "node dist/main.js",
    "start:api:dev": "cross-env NODE_ENV=development nest start --watch --entryFile api/main",
    "start:api:prod": "cross-env NODE_ENV=production nest start node dist/api/main",
    "start:worker": "node dist/worker/main.js",
    "start:worker:dev": "cross-env NODE_ENV=development nest start --watch --entryFile worker/main",
    "start:worker:prod": "cross-env NODE_ENV=production nest start node dist/worker/main",
    "start:all:dev": "concurrently -k -n api,worker -c blue,magenta \"npm run start:api:dev\" \"npm run start:worker:dev\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed:users": "ts-node -r tsconfig-paths/register scripts/seed-users.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.925.0",
    "@aws-sdk/client-secrets-manager": "^3.930.0",
    "@aws-sdk/lib-storage": "^3.925.0",
    "@aws-sdk/s3-request-presigner": "^3.910.0",
    "@deepgram/sdk": "^4.11.2",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.63.0",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "ioredis": "^5.8.2",
    "joi": "^18.0.1",
    "luxon": "^3.7.2",
    "openai": "^6.5.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "twilio": "^5.9.0",
    "typeorm": "^0.3.26",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.18.3",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "cross-env": "^10.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
